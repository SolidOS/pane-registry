  "bugs": {
    "url": "https://github.com/solid/chat-pane/issues"
  },
  "homepage": "https://github.com/solid/chat-pane",
  "dependencies": {
    "rdflib": "^1.2.3"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "standard": "^14.3.4"
  },
  "husky": {
    "hooks": {
