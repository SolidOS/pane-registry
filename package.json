{
  "name": "pane-registry",
  "version": "2.4.27",
  "description": "A registry for Solid UI panes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint 'src/*.ts'",
    "lint-fix": "eslint 'src/*.ts' --fix",
    "test": "npm run lint && npm run prepare",
    "prepublishOnly": "npm test",
    "postversion": "git push origin main --follow-tags"
  },
  "keywords": [
    "solid",
    "ui",
    "panes",
    "registry"
  ],
  "author": "Tim Berners-Lee <timbl@mit.edu>",
  "license": "MIT",
  "dependencies": {
    "rdflib": "^2.2.37",
    "solid-logic": "^3.0.8-eadd741b"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  }
}
